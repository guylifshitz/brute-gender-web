      <etymology>
        <etym>
          <xml><i>(<innerLink ref="#nom commun 1">nom 1</innerLink>)</i><century>xii</century> du bas latin <foreignWord lang="la" sense="chat (domestique)">cattus</foreignWord>, gaulois <i>cattos</i>. le classique <i><innerLink ref="felis#la">felis</innerLink></i> désignait le <innerLink ref="chat sauvage d'europe">chat sauvage d'europe</innerLink> (<i><innerLink ref="felis silvestris silvestris">felis silvestris silvestris</innerLink></i>).</xml>
          <txt>(xiie siècle) du bas latin cattus  (chat (domestique)), gaulois cattos. le classique felis désignait le chat sauvage d'europe (felis silvestris silvestris).</txt>
        </etym>
        <etym>
          <xml><i>(<innerLink ref="#nom commun 2">nom 2</innerLink>)</i><century>xiii</century> probablement de l'ancien français <foreignWord lang="fro" sense="chat-château">chat</foreignWord>.</xml>
          <txt>(xiiie siècle) probablement de l'ancien français chat  (chat-château).</txt>
        </etym>
        <etym>
          <xml><i>(<innerLink ref="#nom commun 3">nom 3</innerLink>)</i><date>1997</date> emprunté à l'anglais <foreignWord lang="en" sense="discussion, bavardage">chat</foreignWord>.</xml>
          <txt>(1997) emprunté à l'anglais chat  (discussion, bavardage).</txt>
        </etym>
      </etymology>
